{
  "version": "1.0.0",
  "imports": {
    "$src/": "./src/",
    "$std/": "https://deno.land/std@0.224.0/",
    "$zod/": "https://deno.land/x/zod@v3.23.8/"
  },
  "tasks": {
    "start": "deno run --env --allow-net --allow-read --allow-env --watch src/main.ts",
    "run": "docker compose up"
  },
  "fmt": {
    "indentWidth": 4,
    "lineWidth": 140,
    "semiColons": false,
    "singleQuote": true,
    "useTabs": true
  }
}